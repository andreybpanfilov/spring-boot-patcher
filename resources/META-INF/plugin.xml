    <name>SpringBoot Patcher</name>
    <version>1.0</version>
    <vendor email="andrew@panfilov.tel" url="https://github.com/andreybpanfilov/spring-boot-patcher">Andrey B.Panfilov</vendor>

    <description><![CDATA[
      Shares spring-boot-maven-plugin settings with IDEA Runner
    ]]></description>

    <change-notes><![CDATA[

    ]]>
    </change-notes>

    <idea-version since-build="203.0"/>

    <depends>com.intellij.modules.platform</depends>
