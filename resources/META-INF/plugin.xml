    <depends>org.jetbrains.idea.maven</depends>
    <depends>org.jetbrains.idea.maven.ext</depends>
    <depends>org.jetbrains.idea.maven.model</depends>


    <extensions defaultExtensionNs="com.intellij">
        <java.programPatcher implementation="tel.panfilov.intellij.plugin.springboot.SpringBootPatcher"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>

</idea-plugin>